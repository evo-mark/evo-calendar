{
  "name": "@evomark/evo-calendar",
  "version": "4.0.0",
  "description": "A calendar and date picker plugin for Vue.js.",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "contributors": [
    {
      "name": "Craig Riley",
      "email": "c.riley@evomark.co.uk"
    },
    {
      "name": "Nathan Reyes",
      "email": "nathanreyes@me.com"
    }
