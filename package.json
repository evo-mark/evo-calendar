{
  "name": "@evomark/evo-calendar",
  "type": "module",
  "version": "4.0.0",
  "description": "A calendar and date picker plugin for Vue.js.",
  "contributors": [
    {
      "name": "Craig Riley",
      "email": "c.riley@evomark.co.uk"
    },
    {
      "name": "Nathan Reyes",
      "email": "nathanreyes@me.com"
    }
  ],
  "repository": "https://github.com/evo-mark/evo-calendar",
  "homepage": "https://vcalendar.io",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "jsdelivr": "dist/iife/index.js",
  "unpkg": "dist/iife/index.js",
  "types": "dist/types/src/index.d.ts",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "plugin",
    "calendar",
    "v-calendar",
    "datepicker",
    "date picker"
  ],
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": {
        "node": "./dist/mjs/index.mjs",
        "default": "./dist/es/index.js"
      },
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "tsx build/build.ts",
    "build:docs": "cd docs && yarn build",
    "build:all": "yarn build && yarn build:docs",
    "test": "vitest",
    "lint": "eslint . --ext .js,.vue",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,vue}\""
  },
  "dependencies": {
    "@types/lodash": "^4.17.13",
    "@types/resize-observer-browser": "^0.1.11",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "lodash-es": "^4.17.21",
    "vue-screen-utils": "1.0.0-beta.13"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.0.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/compiler-sfc": "^3.5.12",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^15.2.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-nested": "^4.2.3",
    "postcss-simple-vars": "^7.0.1",
    "postcss-url": "^10.1.3",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.36.0",
    "tslib": "^2.8.1",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "vue": "^3.5.12",
    "vue-tsc": "^1.8.27"
  }
}
